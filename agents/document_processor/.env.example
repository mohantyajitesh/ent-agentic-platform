# Document Processor Agent - Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env files to version control

# ==============================================
# AWS Configuration
# ==============================================
AWS_REGION=us-east-1

# ==============================================
# Agent Configuration
# ==============================================
# Agent runtime ID (without the full ARN - account ID is discovered via STS)
# Example: document_processor-dvU2Bk3C7c
DOCUMENT_PROCESSOR_AGENT_ID=

# Memory ID for Bedrock AgentCore memory
# Example: document_processor_mem-C2y8M7BwiW
BEDROCK_AGENTCORE_MEMORY_ID=

# ==============================================
# Document Processing Configuration
# ==============================================
TEXTRACT_TIMEOUT_MINUTES=10
SIGNATURE_CONFIDENCE_THRESHOLD=0.85
MAX_VISION_FILE_SIZE_MB=20
CLAUDE_MAX_TOKENS=4096
CLAUDE_MODEL_ID=us.anthropic.claude-sonnet-4-20250514-v1:0
